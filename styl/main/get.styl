.get-list
	width 100%
	max-width 860px
	margin: 0 auto
	padding-left: 10px
	list-style none
	background-color: rgba(#BB863D,1)
	padding 20px 20px 20px 30px
	box-shadow: 0 2px 5px rgba(#BB863D,.5)
	border-radius: 5px
	li
		color: #FDFEDE
		margin-top: 10px
		position relative
		padding-left: 15px
		&:before
			content: ''
			border-radius: 2px
			position: absolute
			top: 8px
			left 0
			width 9px
			height 9px
			background-color: #FDFEDE